{
  "id": "Room1Agent",
  "modules": [
    {
      "module_id": "room",
      "type": "simulator",
      "model": {
        "type": "fmu",
        "path": "models/SimpleRoom.fmu"
      },
      "measurement_uncertainty": 0.0001,
      "t_sample": 50,
      "save_results": true,
      "inputs": [
        {
          "name": "Q_flow_heat",
          "value": 0
        },
        {
          "name": "T_oda",
          "value": 273.15
        }],
      "outputs": [
        {
          "name": "T_air"
        }
      ]
    },
    {
      "module_id": "ComLocal",
      "type": "local",
      "subscriptions": ["PIDAgent", "TRYSensorAgent"],
      "parse_json": true
    }
  ]
}